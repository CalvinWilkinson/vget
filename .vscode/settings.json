{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/bin": true,
        "**/*.log": true
    },
    "cSpell.words": [
        "CASL",
        "Kinson",
        "creds",
        "jsxdev",
        "linebreak",
        "nodir",
        "paren",
        "parens",
        "postclean",
        "prerebuild",
        "readonly",
        "repos",
        "vercel",
        "vget"
    ],
	"[typescript]": {
		"editor.insertSpaces": false,
		"editor.detectIndentation": false,
        "editor.autoIndent": "full",
		"editor.tabSize": 4
	},
}